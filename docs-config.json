{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["./"],
		"exclude": ["./node_modules"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"opts": {
		"destination": "./public/docs/",
		"recurse": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
